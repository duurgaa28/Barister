<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Barister" parallel="tests" thread-count="2">
	<listeners>
		<listener class-name="Util.ReportUtil"></listener>
	</listeners>
	<test name="Search Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.SearchTest"></class>
		</classes>
	</test>
	<test name="Register Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.RegisterTest"></class>
		</classes>
	</test>
     <test name="Password Validation Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.SearchTest"></class>
		</classes>
	</test>
	<test name="SignIn Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.SignInTest"></class>
		</classes>
	</test>
</suite>